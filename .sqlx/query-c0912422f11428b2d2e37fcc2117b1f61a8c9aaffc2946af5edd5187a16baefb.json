  "query": "\n            SELECT \n                ls.*,\n                'Team ' || SUBSTRING(ls.team_id::text, 1, 8) as team_name,\n                '#4F46E5' as team_color\n            FROM league_standings ls\n            WHERE ls.season_id = $1\n            ORDER BY ls.points DESC, (ls.wins * 3 + ls.draws) DESC, ls.wins DESC\n            ",
