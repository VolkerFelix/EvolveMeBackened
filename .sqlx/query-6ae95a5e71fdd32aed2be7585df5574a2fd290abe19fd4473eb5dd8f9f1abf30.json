  "query": "\n        SELECT \n            tm.team_id,\n            tm.user_id,\n            tm.status,\n            COALESCE(ua.stamina, 0) as stamina,\n            COALESCE(ua.strength, 0) as strength\n        FROM team_members tm\n        LEFT JOIN user_avatars ua ON tm.user_id = ua.user_id\n        WHERE tm.team_id = ANY($1)\n        ",
